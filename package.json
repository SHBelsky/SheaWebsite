    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^4.3.0",
    "img-loader": "^3.0.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "raw-loader": "^4.0.0",
    "style-loader": "^1.1.3",
    "sw-precache-webpack-plugin": "^1.0.0",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
